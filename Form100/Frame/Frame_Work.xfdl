		obj.set_value(e.prevalue);
		return;
	}
	if(this.gfn_isNull(this.div_intm.edt_hndsetSeq.text)) {
		this.gfn_showMsgBox("NBSS-I000", "단말기 일련번호를 입력하여 주십시오.");
		obj.set_value(e.prevalue);
		return;
	}
	if(this.gfn_isNull(this.div_spnsr.msk_custBrdnAmt.value)) {
		this.gfn_showMsgBox("NBSS-I000", "고객부담금이 없습니다.");
		obj.set_value(e.prevalue);
		return;
	}
	// 요금제
	if (this.gfn_isNull(this.div_ppl.fn_getSelProdId()) ) 
	{
		this.gfn_showMsgBox("NBSS-I000", "요금제를 입력하여 주십시오.");
		return;
	}
	
	this.gfn_enableCtrls(this.div_buy, "sta_prepayAmt,sta_instaAmt");		

	var sSalePlcyId 		= "";	//기기판매정책아이디
	var sIntmSsaleTypeCd 	= "";	//기기특판유형코드
