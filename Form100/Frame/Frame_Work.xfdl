this.fn_retvSalePlcyInfo = function (pIntmSalePlcyId)
{
	//trace("===판매정책정보조회 fn_retvSalePlcyInfo===");	
	//trace("ASIS:기기 판매번호정보 조회 [/Intm/IntmMkt/IntmMdlSlsOptnMgmt/inquireIntmSlsOptionDetail]");
	
	//판매조직아이디
	if(this.gfn_isNull(this.div_sale.edt_salorgId.text) ) {
		this.gfn_showMsgBox("NBSS-I000", "판매점ID를 먼저 입력하여 주십시오.");
		this.div_sale.edt_salorgId.setFocus();
		return;
	}	
	
	//기기판매정책아이디
	if(this.gfn_isNull(pIntmSalePlcyId)) {
		this.gfn_showMsgBox("NBSS-I000", "판매번호를 입력하여 주십시오.");
		this.div_intm.edt_saleNo.setFocus();
		return;
	}	

	//기기판매가입구분코드	
	var sIntmSaleSbscDivCd = "";
	if(this.cbo_icgType.value == this.GC_ICG_TYPE_01) {
		//우수기변
		sIntmSaleSbscDivCd = "2";
	}else if(this.cbo_icgType.value == this.GC_ICG_TYPE_17) {
		//휴대폰기변
		sIntmSaleSbscDivCd = "3";
	}else {
		return;
	}	

	this.gfn_initDS(this.ds_mblSalePlcyInfoIn , true);	
	this.gfn_initDS(this.ds_mblSalePlcyInfo	  , false);
	
	this.ds_mblSalePlcyInfoIn.setColumn(0, "intmSalePlcyId" 	, pIntmSalePlcyId);	
	this.ds_mblSalePlcyInfoIn.setColumn(0, "saleDate"	   		, this.gfn_getToday());
	this.ds_mblSalePlcyInfoIn.setColumn(0, "saleOrgId"	   		, this.div_sale.edt_salorgId.text);
	this.ds_mblSalePlcyInfoIn.setColumn(0, "intmSaleSbscDivCd"	, sIntmSaleSbscDivCd);

	////trace("===INPUT===>\n"+this.ds_mblSalePlcyInfoIn.saveXML());		
	
	var sInDs = "ds_mblSalePlcyInfoIn=ds_mblSalePlcyInfoIn";			
	var sOutDs = "ds_mblSalePlcyInfo=ds_mblSalePlcyInfo";		
	
	this.gfn_transaction("callback_retvSalePlcyInfo", "ORD::/ORD/PipelineSVC/NSEN/SalePlcy/mobile/PlcyRetv/PL_MblSalePlcyInfoAdm.retrieveSalePlcyInfo", sInDs, sOutDs, "fn_callback", false, true);

}
