/**
 * @description	사은품 Combo 변경시
 */
this.cbo_aprgds_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	this.div_buy.msk_aprgds.set_value();
	this.div_buy.edt_aprgds.set_value();
				
	switch(obj.value) 
	{
		case "01":	//없음
			this.gfn_showCtrls(this.div_buy, "", "msk_aprgds,edt_aprgds");
			break;

		case "02":	//현금 
			this.gfn_showCtrls(this.div_buy, "msk_aprgds", "edt_aprgds");
			break;
						
		case "03":	//수기입력 
			this.gfn_showCtrls(this.div_buy, "edt_aprgds", "msk_aprgds");
			break;
									
		default:
			break;				
	}	
}
