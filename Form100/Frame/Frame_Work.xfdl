	//입력LOCK 체크
	if(this.GC_INPUT_LOCK == "Y") {
	
		var msg = "기변종류 변경시 현재까지 작업된 내용은 삭제됩니다.\n변경하시겠습니까?";

		if(this.gfn_showMsgBox("NBSS-Q000", msg) == "YES" ){
			
			this.GC_INPUT_LOCK = "N";	
			
