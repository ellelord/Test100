				,ds_spnsrInfo: 			this.ds_pop_spnsrInfo					//스폰서정보 dataset
				,pv_fornYn:     		sFornYn									//외국인여부
				,pv_saleCmpnId:			this.parent.ds_contBasInfo.getColumn(0, "saleCmpnId") 				//판매회사코드					
				,pv_jobCd:  			this.parent.ds_custSumryInfo.getColumn(0, "jobCd")					//직업코드
				,pv_custNm:  			this.parent.ds_custSumryInfo.getColumn(0, "custNm")					//고객명
