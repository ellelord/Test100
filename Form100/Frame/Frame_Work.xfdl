/**
 * @description    transaction callback
 * @param 	 {string} sTrId Transaction ID
 * @param 	 {number} nErrorCode 에러코드
 * @param 	 {string} sErrorMsg 에러메세지
 */
this.fn_callback = function (sTrId, nErrorCode, sErrorMsg)
{
	switch (sTrId) 
	{	
		//화면초기화 START ->	
		
		//공통코드 조회 
		case "setCommCode":
			if( nErrorCode < 0 ) return;
			
			//trace("기기변경상세코드===>\n"+this.ds_comn_icgType.saveXML());
			//trace("보험사코드===>\n"+this.ds_comn_icomCd.saveXML());	
			//trace("청구서발송유형코드===>\n"+this.ds_comn_dspTypeCd.saveXML());	
			
			//01:우수고객기변 17:휴대폰기변 11:일반기변 30:AS기변 54:olleh폰안심플랜기변 38:USIM변경
			var sFilter = "cd=='"+this.GC_ICG_TYPE_01+"'";
			sFilter += " || cd=='"+this.GC_ICG_TYPE_11+"'";
			sFilter += " || cd=='"+this.GC_ICG_TYPE_17+"'";
			sFilter += " || cd=='"+this.GC_ICG_TYPE_30+"'";
			sFilter += " || cd=='"+this.GC_ICG_TYPE_54+"'";
			sFilter += " || cd=='"+this.GC_ICG_TYPE_38+"'";
			this.ds_comn_icgType.filter(sFilter);
			
			//처리유형 초기값 설정
			this.cbo_icgType.set_index(0);
			
			//사전체크 버튼 
			this.gfn_enableCtrls(this, "btn_bfacChk", "btn_dtlReslt");			
		
			break;			
			
		//시스템코드 조회	
		case "setSysCode":
			if( nErrorCode < 0 ) return;
			
			//trace("시스템코드===>\n"+this.ds_sysCdAbleUsimCost.saveXML());
			//trace("시스템코드===>\n"+this.ds_sysCdWktIpadPrdc.saveXML());
			//trace("시스템코드===>\n"+this.ds_sysCdIcgSspnAplyYn.saveXML());
			//trace("시스템코드===>\n"+this.ds_sysCdOrgnChngShopYn.saveXML());
			//trace("시스템코드===>\n"+this.ds_sysCdOpenRstrMsgSbst.saveXML());
			//trace("시스템코드===>\n"+this.ds_sysCdAbleUsimCost.saveXML());
			//trace("시스템코드===>\n"+this.ds_sysCdRetailCrclUsimOrgn.saveXML());
						
