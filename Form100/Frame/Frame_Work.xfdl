				,pv_intmSeq:			this.div_intm.edt_hndsetSeq.text 		//기기일련번호
				,pv_hndsetAmt:  		iHndsetAmt								//단말기금액(할부원금)
				,pv_hndsetDc:			this.div_spnsr.msk_hndsetDc.value		//단말기할인	
