	//입력LOCK 체크
	if(this.GC_INPUT_LOCK == "Y") {
	
		var msg = "처리유형 변경시 현재까지 작업된 내용은 삭제됩니다.\n변경하시겠습니까?";

		if(this.gfn_showMsgBox("NBSS-Q000", msg) == "YES" ){
		
			this.GC_INPUT_LOCK = "N";		
			
			//사전체크 Dataset Clear
			this.gfn_initDS(this.ds_bfacChkDisplay, false);
			//임시저장 Dataset Clear		
			this.gfn_initDS(this.ds_icgTmpInfoOut, false);			
			
			//모든 컴포넌트 Enable/Disable
			this.ofn_enableAllComp(false);
			//모든 컴포넌트 Data Clear
			this.ofn_clearAllCompData();		
				
			//사전체크 버튼 
			this.gfn_enableCtrls(this, "btn_bfacChk", "btn_dtlReslt");	
		}else {
			obj.set_value(e.prevalue);
			return;
		}
	}

	switch(obj.value)
	{
		//일반
		case "01":
			//기변종류 필터링 
			//01:우수고객기변 17:휴대폰기변 11:일반기변 30:AS기변 54:olleh폰안심플랜기변 38:USIM변경
			var sFilter = "cd=='"+this.GC_ICG_TYPE_01+"'";
			sFilter += " || cd=='"+this.GC_ICG_TYPE_11+"'";
			sFilter += " || cd=='"+this.GC_ICG_TYPE_17+"'";
			sFilter += " || cd=='"+this.GC_ICG_TYPE_30+"'";
			sFilter += " || cd=='"+this.GC_ICG_TYPE_54+"'";
			sFilter += " || cd=='"+this.GC_ICG_TYPE_38+"'";
			this.ds_comn_icgType.filter(sFilter);			
			break;
				
		//스마트개통
		case "02":
			//기변종류 필터링
			var sFilter = "cd=='"+this.GC_ICG_TYPE_01+"'";
			this.ds_comn_icgType.filter(sFilter);	
				
			//this.gfn_showMsgBox("NBSS-I000", "===스마트개통I3===");
			
			//메세지: 조회된 전화번호와 서식지 접수된 전화번호가 다릅니다. 확인하세요.
			break;
							
		//임시저장정보
		case "03":	
			//기변종류 필터링			
			var sFilter = "cd=='"+this.GC_ICG_TYPE_01+"'";
			sFilter += " || cd=='"+this.GC_ICG_TYPE_17+"'";
			this.ds_comn_icgType.filter(sFilter);	
						
			//임시저장정보조회
			this.fn_retvIcgTmpInfo(); 						

			break;
			
		default:
			break;			
	}	
}
/**
 * @description	기변종류 Combo값 변경시
 */
this.cbo_icgType_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{	
	//trace("===기변종류===>\n"+obj.value);
/*	
01	우수고객기변
17	휴대폰기변
11	일반기변
30	AS기변
54	olleh폰안심플랜기변
38	USIM변경
