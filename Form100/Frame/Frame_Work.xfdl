 */
this.btn_spnsr_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	//조직ID
	if(this.gfn_isNull(this.div_sale.edt_salorgId.text)) {
		this.gfn_showMsgBox("NBSS-I000", "판매점을 입력하여 주십시오.");
		return;
	}
	//단말기모델ID
	if(this.gfn_isNull(this.div_intm.edt_hndsetModelId.text)) {
		this.gfn_showMsgBox("NBSS-I000", "단말기모델을 입력하여 주십시오.");
		return;
	}
	//단말기일련번호 
	if(this.gfn_isNull(this.div_intm.edt_hndsetSeq.text)) {
		this.gfn_showMsgBox("NBSS-I000", "단말기일련번호를 입력하여 주십시오.");
		return;
	}	
	// 요금제
	if (this.gfn_isNull(this.div_ppl.fn_getSelProdId()) ) 
	{
		this.gfn_showMsgBox("NBSS-I000", "요금제를 입력하여 주십시오.");
		return;
	}		

	//판매약정유형구분코드
	var sIntmSaleEngtTypeDtlCd = "";
	if(this.gfn_isNull(this.div_intm.edt_saleNo.text)) {
		sIntmSaleEngtTypeDtlCd = this.ds_mblBasSalePlcyIdOut.getColumn(0, "intmSaleEngtTypeDtlCd");
	}else {
		sIntmSaleEngtTypeDtlCd = this.ds_mblSalePlcyInfo.getColumn(0, "intmSaleEngtTypeDtlCd");
	}
	
	var sIntmSalePayotTypeCd = this.chk_hndsetInsl.value == true ?"2":"1";
	
	//temp
	//sIntmSaleEngtTypeDtlCd = "KS1";
		
	trace("pv_sModelId==>"+this.div_intm.edt_hndsetModelId.text);
	trace("pv_sOrgId==>"+this.div_sale.edt_salorgId.text);
	trace("pv_nIntmSaleAmt==>"+this.div_spnsr.msk_custBrdnAmt.value );
	trace("pv_sWrkjobTrtCd==>"+"HCN");
	trace("pv_sSvcContDivCd==>"+this.ds_modelListOut.getColumn(0, "mobilGenTypeCd"));
	trace("pv_sProdId==>"+this.div_ppl.fn_getSelProdId());
	trace("pv_sCustTypeCd==>"+this.parent.ds_custSumryInfo.getColumn(0, "custTypeCd"));
	trace("pv_sCustIdfyNoTypecd==>"+this.parent.ds_custSumryInfo.getColumn(0, "custIdfyNoTypeCd")	);
	trace("pv_sCustIdfyNo==>"+this.parent.ds_custSumryInfo.getColumn(0, "custIdfyNo"));
	trace("pv_sCustNm==>"+this.parent.ds_custSumryInfo.getColumn(0, "custNm"));
	trace("pv_sSaleCmpnId==>"+this.parent.ds_contBasInfo.getColumn(0, "saleCmpnId"));
	trace("pv_sHndchnKind==>"+this.cbo_icgType.value);
	trace("pv_sSaleEngtTypeDivCd==>"+sIntmSaleEngtTypeDtlCd);	
	trace("pv_sIntmSalePayotTypeCd==>"+sIntmSalePayotTypeCd );
	trace("pv_sForeignYn==>"+"N");

	var oArg = { pv_sModelId:				this.div_intm.edt_hndsetModelId.text							//단말기모델ID				//*필수
			    ,pv_sOrgId:					this.div_sale.edt_salorgId.text									//조직ID					//*필수
			    ,pv_nIntmSaleAmt:			this.div_spnsr.msk_custBrdnAmt.value  							//단말 판매가				//*필수
			    ,pv_sWrkjobTrtCd:			"HCN"															//업무구분코드			//*필수 //NAC : 개통 HCN : 기변
			    ,pv_sSvcContDivCd:			this.ds_modelListOut.getColumn(0, "mobilGenTypeCd")				//서비스계약구분코드
			    ,pv_sProdId:				this.div_ppl.fn_getSelProdId()									//상품아이디				//*필수
			    ,pv_sCustTypeCd:			this.parent.ds_custSumryInfo.getColumn(0, "custTypeCd")			//고객유형코드 			//*필수
			    ,pv_sCustIdfyNoTypecd:		this.parent.ds_custSumryInfo.getColumn(0, "custIdfyNoTypeCd")	//고객식별번호유형코드	//*필수
			    ,pv_sCustIdfyNo:			this.parent.ds_custSumryInfo.getColumn(0, "custIdfyNo")			//고객식별번호			//*필수
			    ,pv_sCustNm:				this.parent.ds_custSumryInfo.getColumn(0, "custNm")				//고객명					//*필수
			    //,pv_sSaleCmpnId:			this.parent.ds_contBasInfo.getColumn(0, "saleCmpnId")			//판매회사아이디			//*필수
			    ,pv_sSaleCmpnId:			"KT"															//판매회사아이디			//*필수
			    ,pv_sHndchnKind:			this.cbo_icgType.value											//기기변경코드			//*필수
			    ,pv_sSaleEngtTypeDivCd:		sIntmSaleEngtTypeDtlCd  										//판매약정유형구분코드	//*필수
			    ,pv_sIntmSalePayotTypeCd: 	sIntmSalePayotTypeCd											//기기판매지불유형코드
			    ,pv_sForeignYn:				"N"
			   }; 
	
	var sOption = "popupType=modalsync";  
	
	var sRtnVal = this.gfn_openPopup("popup_spnsr", "NSEN::ONSEN1000PU.xfdl", oArg, sOption, "fn_callbackPop");
		
	//trace("popup_spnsr==sRtnVal==>"+sRtnVal);

	if(!this.gfn_isNull(sRtnVal)) {
		
		//스폰서 Data clear
		this.ofn_clearSpnsrData();	
				
		//할부정보 Data clear
		this.ofn_clearInslMonsData();		
		
		//수납대상금액 계산	
		this.ofn_payAmount(); 			
			
		this.ds_pop_spnsrInfo.assign(sRtnVal);
		
		//trace("===ds_pop_spnsrInfo====>\n"+this.ds_pop_spnsrInfo.saveXML());
		
		if(this.gfn_parseInt(this.ds_pop_spnsrInfo.getColumn(0, "dcSuprtAmtIntm")) > 0 ) {
		
			if(this.gfn_parseInt(this.ds_pop_spnsrInfo.getColumn(0, "dcSuprtAmtIntm")) > this.gfn_parseInt(this.div_buy.msk_rmnyAmt.value) ) {
			
				this.gfn_showMsgBox("NBSS-I000", "단말기가격보다 큰 단말기할인 금액은 불가합니다.");
					
				//스폰서 Data clear
				this.ofn_clearSpnsrData();	
				return;
			}
		}
		
		var iTotDcAmt = 0;
		
		if(this.ds_pop_spnsrInfo.getColumn(0, "saleEngtOptnCd") == "KD" 			
				|| this.ds_pop_spnsrInfo.getColumn(0, "saleEngtOptnCd") == "TD" 
				|| this.ds_pop_spnsrInfo.getColumn(0, "saleEngtOptnCd") == "ID" ) {
		
			if(this.ds_pop_spnsrInfo.getColumn(0, "saleEngtOptnCd") == "KD") { 			//베이직코스
				iTotDcAmt = this.gfn_parseInt(this.ds_pop_spnsrInfo.getColumn(0, "dcSuprtAmtBasic"))		//베이직코스지원금액
							+ this.gfn_parseInt(this.ds_pop_spnsrInfo.getColumn(0, "dcSuprtAmtMnkg"))		//제조사 지원금액
							+ this.gfn_parseInt(this.ds_pop_spnsrInfo.getColumn(0, "dcSuprtAmtStor"));		//매장지원금	
			}else if(this.ds_pop_spnsrInfo.getColumn(0, "saleEngtOptnCd") == "TD") {	//심플코스
				iTotDcAmt = this.gfn_parseInt(this.ds_pop_spnsrInfo.getColumn(0, "dcSuprtAmtSimple"))		//심플코스 지원금액
							+ this.gfn_parseInt(this.ds_pop_spnsrInfo.getColumn(0, "dcSuprtAmtMnkg"))		//제조사 지원금액
							+ this.gfn_parseInt(this.ds_pop_spnsrInfo.getColumn(0, "dcSuprtAmtStor"));		//매장지원금				
			}else if(this.ds_pop_spnsrInfo.getColumn(0, "saleEngtOptnCd") == "ID") {	//IOT
				iTotDcAmt = this.gfn_parseInt(this.ds_pop_spnsrInfo.getColumn(0, "dcSuprtAmtSimple"));		//심플코스 지원금액		
			}
			
			if(iTotDcAmt > this.gfn_parseInt(this.div_buy.msk_rmnyAmt.value) ) {
			
				if(!(this.cbo_icgType.value == this.GC_ICG_TYPE_01 && this.chk_hndsetInsl.value == true) ) {
					var sMsg = "";
					
					if(this.ds_pop_spnsrInfo.getColumn(0, "saleEngtOptnCd") == "ID") {
						sMsg = "IOT";
					}
					
					sMsg += "단말기가격보다 큰 단말기할인 금액은 불가합니다.";
					
					this.gfn_showMsgBox("NBSS-I000", sMsg);

					//스폰서 Data clear
					this.ofn_clearSpnsrData();	
					return;					
				}
			}
		}
		
		//스폰서명 
		this.div_spnsr.edt_spnsr.set_value(this.ds_pop_spnsrInfo.getColumn(0, "spnsrNm"));
		
		var iSuccAmt = this.gfn_parseInt(this.div_spnsr.msk_custBrdnAmt.value) - this.gfn_parseInt(this.ds_pop_spnsrInfo.getColumn(0, "dcSuprtAmtIntm"));
		
		if(iTotDcAmt > 0) {
		
			iSuccAmt = this.gfn_parseInt(this.div_spnsr.msk_custBrdnAmt.value) - iTotDcAmt;
		}
		
		if(iSuccAmt < 0) {
			//단말기할인 
			this.div_spnsr.msk_hndsetDc.set_value(this.div_spnsr.msk_custBrdnAmt.value);
		}else {
			//단말기할인 
			this.div_spnsr.msk_hndsetDc.set_value(this.gfn_parseInt(this.ds_pop_spnsrInfo.getColumn(0, "dcSuprtAmtIntm")));
			if(iTotDcAmt > 0) {
				this.div_spnsr.msk_hndsetDc.set_value(iTotDcAmt);
			}
		}

		//수납대상금액 계산	
		this.ofn_payAmount(); 				
	}
}
