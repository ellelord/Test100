/**
 * @description	사전체크 버튼 클릭시
 */
this.btn_bfacChk_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{	
	//기변종류
	this.cbo_icgType.value = this.cbo_icgType.value;

	//사전체크LOCK
	if(this.GC_CHECK_LOCK == "Y") {
		return;
	}
	//temp
	if(this.cbo_trtType.value == "02") {
		this.gfn_showMsgBox("NBSS-I000", "===스마트개통 기능 I3===");
		return;
	}
	
	
	//AS기변,판매후동종,판매후이종
	if(this.cbo_icgType.value == this.GC_ICG_TYPE_30) {
		this.fv_asIcgDivCd = "2"; 	
	}else {
		this.fv_asIcgDivCd = ""; 
	}
	
	//사전체크 Display Dataset Clear
	this.gfn_initDS(this.ds_bfacChkDisplay, false);		

	//기본체크 START ->
	
	//처리유형 null체크 
	if(this.gfn_isNull(this.cbo_trtType.value) ) {
		this.gfn_showMsgBox("NBSS-I000", "처리유형을 선택해 주십시오.");
		this.cbo_trtType.setFocus();
		return;
	}
	
	//기변종류 null체크 
	if(this.gfn_isNull(this.cbo_icgType.value) ) {
		this.gfn_showMsgBox("NBSS-I000", "기변종류를 선택해 주십시오.");
		this.cbo_icgType.setFocus();
		return;
	}
		
	//단말기할부 체크 
	if(!(this.cbo_icgType.value == this.GC_ICG_TYPE_01 || this.cbo_icgType.value == this.GC_ICG_TYPE_17) ) {	//우수고객기변 || 휴대폰기변
		//할부정보 Data clear
		this.ofn_clearInslMonsData();
	}

	//개인정보활용 체크
	if(this.cbo_icgType.value != this.GC_ICG_TYPE_01) {	//우수고객기변		
		this.rdo_indvInfoYn.set_value();
	}
	
	//기본체크 END ->
	
	//메시지 체크 START ->
	
	//휴일정보 세팅 
	this.parent.fn_setHoliInfo();			
	
	//휴일/영업시간 외 체크 
	if(!(this.cbo_icgType.value == this.GC_ICG_TYPE_01 || this.cbo_icgType.value == this.GC_ICG_TYPE_17)) {
		if(this.GC_TODAY_HOLI_YN == "Y") {
			if(this.GC_HOLI_USER_YN == "N") {
				this.gfn_showMsgBox("NBSS-I000", "휴일업무는 우수고객기변 또는 휴대폰기변만 가능합니다. 확인하세요.");
				this.cbo_icgType.set_index(0);
				return;
			}
		}else {
			if(this.GC_WORK_TIME_YN == "N" && this.GC_POSBL_ORG_YN == "N" ) {

				this.gfn_showMsgBox("NBSS-I000", "업무시간 외 업무는 우수고객기변 또는 휴대폰기변만 가능합니다. 확인하세요.");
				this.cbo_icgType.set_index(0);
				return;
			}
		}
	}
	
	//대리점일 경우 
	if(this.ds_orgInfoDtlLogin.getColumn(0, "orgRoleDivCd") == "02") {	//02:유통조직  	
		if(this.cbo_icgType.value == this.GC_ICG_TYPE_01 || this.cbo_icgType.value == this.GC_ICG_TYPE_17) {	//우수고객기변 || 휴대폰기변

			if(this.ds_orgInfoDtlLogin.getColumn(0, "rwrdIntmChgPosblYn") != "Y") {
				this.gfn_showMsgBox("NBSS-I000", "기기변경 가능한 대리점이 아닙니다.");
				return;
			}			
		}		
	}	
	
	//쇼기본형 베이스 해지시 위약금 청구 로직 구현필요 
	if(this.ds_spnsrContInfo.rowcount > 0 ) {

		if(this.ds_spnsrContInfo.getColumn(0,"saleEngtTypeDivCd") == "FND" ) {
		
			var strMsg  = "쇼킹스폰서 기본형 가입고객 입니다.";
			    strMsg += " 우수기변 처리를 하면 약정기간이 만료되지";
			    strMsg += " 않은 경우 위약금이 발생하고 후청구 처리됩니다.";

			this.gfn_showMsgBox("NBSS-I000", strMsg + "확인하세요.");
		}
	}
		
	//메시지 체크 END ->
	
	
	//서비스 호출 START ->
	
	//기본판매정책아이디조회  
	this.fn_retvMblBasSalePlcyId();	
	
	
	//보증보험정보조회 
	//temp//this.fn_retvGinsInfo();			

	//일반기변	|| olleh폰안심플랜기변 
	if(this.cbo_icgType.value == this.GC_ICG_TYPE_11 || this.cbo_icgType.value == this.GC_ICG_TYPE_54) {		
		//기기보험가입정보조회
		this.fn_retvIntmInsurSbscInfo();
	}
	
	//무선대리점개통건수조회
	this.fn_retvWlessAgncyOpnCascnt();
		
	//서비스 호출 END ->
	
	
	//사전체크 dataset 추가 START ->
	
	//대리인 정보체크 
	if(this.cbo_icgType.value != this.GC_ICG_TYPE_38) {	//USIM변경 		
		//I3
		//fn_LglAgnt()
	}	
	
	//실명인증
	if(!(this.cbo_icgType.value == this.GC_ICG_TYPE_01 || this.cbo_icgType.value == this.GC_ICG_TYPE_17) ) {	//우수고객기변 || 휴대폰기변
		this.chk_hndsetInsl.set_value(false);
	}		
	
	//외국인 여권 후불
	if((this.cbo_icgType.value == this.GC_ICG_TYPE_01 || this.cbo_icgType.value == this.GC_ICG_TYPE_17) 
			&& this.parent.ds_svcContSbscSumryInfo.getColumn(0, "contPurpCd") == "R" 	//R:일반
			&& this.parent.ds_custSumryInfo.getColumn(0, "custIdfyNoTypeCd") == "2"		//2:여권번호 
			&& !this.gfn_excpRuleInfo("13@OCSM1010FM") ) { //13@OCSM1010FM: MVNO외국인 여권 후불 할부 개통 권한권한

		var nRow = this.gfn_addRow(this.ds_bfacChkDisplay);
		this.ds_bfacChkDisplay.setColumn(nRow, "msgSbst"		, "외국인 여권 후불 개통/기변 권한자만 기변 가능합니다");
		this.ds_bfacChkDisplay.setColumn(nRow, "trgtAtribSbst"	, "기변불가");
		this.ds_bfacChkDisplay.setColumn(nRow, "resltCd"		, "N");
		this.ds_bfacChkDisplay.setColumn(nRow, "ruleNm"			, "외국인");

		//상세결과 버튼 클릭
		this.btn_dtlReslt_onclick();
		return;	
	}
	
	//프레스티지요금제 가입고객
	if(this.parent.ds_contBasInfo.getColumn(0, "prodId") == "PRSTIGE4G") {
	
		if(!(this.cbo_icgType.value == this.GC_ICG_TYPE_11 || this.cbo_icgType.value == this.GC_ICG_TYPE_30 || this.cbo_icgType.value == this.GC_ICG_TYPE_38) ) {
			
			var nRow = this.gfn_addRow(this.ds_bfacChkDisplay);
			this.ds_bfacChkDisplay.setColumn(nRow, "msgSbst"		, "프레스티지요금제 가입고객은 일반/AS동종기변/USIM변경만 가능");
			this.ds_bfacChkDisplay.setColumn(nRow, "trgtAtribSbst"	, "기변불가");
			this.ds_bfacChkDisplay.setColumn(nRow, "resltCd"		, "N");
			this.ds_bfacChkDisplay.setColumn(nRow, "ruleNm"			, "요금제");
			
			//상세결과 버튼 클릭
			this.btn_dtlReslt_onclick();
			return;				
		}	
	}

	//임직원용 아이패드 요금제(3G + WIFI용)
	var iRow = this.ds_sysCdWktIpadPrdc.findRow("sysCdId", this.parent.ds_contBasInfo.getColumn(0, "prodId"));
	if(iRow > -1 && !this.gfn_excpRuleInfo("9@OHDS1011LY")) { //9@OHDS1011LY: KT업무용 기변 권한		
		var nRow = this.gfn_addRow(this.ds_bfacChkDisplay);
		this.ds_bfacChkDisplay.setColumn(nRow, "msgSbst"		, "임직원용 아이패드요금제 가입고객은 기변 불가\n(권한자만 가능)");
		this.ds_bfacChkDisplay.setColumn(nRow, "trgtAtribSbst"	, "기변불가");
		this.ds_bfacChkDisplay.setColumn(nRow, "resltCd"		, "N");
		this.ds_bfacChkDisplay.setColumn(nRow, "ruleNm"			, "요금제");

		//상세결과 버튼 클릭
		this.btn_dtlReslt_onclick();
		return;				
	}

	//USIM 단독개통 
	if(this.gfn_isNull(this.parent.ds_contBasInfo.getColumn(0, "intmSeq")) ) {
	
		if(this.cbo_icgType.value == this.GC_ICG_TYPE_11) {
			
			if(this.parent.ds_contBasInfo.getColumn(0, "lastSvcContSttusCd") == "S") {	//S:정지
			
				var nRow = this.gfn_addRow(this.ds_bfacChkDisplay);
				this.ds_bfacChkDisplay.setColumn(nRow, "msgSbst"		, "개통 상태가 아니면 기변 불가");
				this.ds_bfacChkDisplay.setColumn(nRow, "trgtAtribSbst"	, "기변불가");
				this.ds_bfacChkDisplay.setColumn(nRow, "resltCd"		, "N");
				this.ds_bfacChkDisplay.setColumn(nRow, "ruleNm"			, "상태");
			
				//상세결과 버튼 클릭
				this.btn_dtlReslt_onclick();
				return;					
			}else {
			
				if(application.G_USER_ENTR_PRSN_INFO == this.GC_USER_ENTR_PRSN) {
	
					if(this.ds_sysCdIcgSspnAplyYn.getColumn(0, "sysCdId") == "Y" && !this.gfn_excpRuleInfo("12@OHDS1011LY") ) { //12@OHDS1011LY": 영업정지기간 일반기변 가능권한
						
						var nRow = this.gfn_addRow(this.ds_bfacChkDisplay);
						this.ds_bfacChkDisplay.setColumn(nRow, "msgSbst"		, "영업정지기간 USIM단독개통은 권한자만 기변 가능합니다.");
						this.ds_bfacChkDisplay.setColumn(nRow, "trgtAtribSbst"	, "기변불가");
						this.ds_bfacChkDisplay.setColumn(nRow, "resltCd"		, "N");
						this.ds_bfacChkDisplay.setColumn(nRow, "ruleNm"			, "USIM단독개통");

						//상세결과 버튼 클릭
						this.btn_dtlReslt_onclick();
						return;										
					}
				}
			}
		}else {		
			var nRow = this.gfn_addRow(this.ds_bfacChkDisplay);
			this.ds_bfacChkDisplay.setColumn(nRow, "msgSbst"		, "USIM 단독 개통은 일반기변만 가능 합니다.");
			this.ds_bfacChkDisplay.setColumn(nRow, "trgtAtribSbst"	, "기변불가");
			this.ds_bfacChkDisplay.setColumn(nRow, "resltCd"		, "N");
			this.ds_bfacChkDisplay.setColumn(nRow, "ruleNm"			, "USIM단독개통");

			//상세결과 버튼 클릭
			this.btn_dtlReslt_onclick();
			return;			
		}
	}
	
	//15일내 개통이력 체크 
	var sOpnCascnt = this.ds_wlessAgncyOpnOut.getColumn(0, "opnCascnt");
	if(sOpnCascnt > 0) {
		
		var nRow = this.gfn_addRow(this.ds_bfacChkDisplay);
		this.ds_bfacChkDisplay.setColumn(nRow, "msgSbst"		, "본 고객님은 최근 " + sOpnCascnt + "회선의 개통 이력이 있는 고객이므로 주의를 요합니다.");
		this.ds_bfacChkDisplay.setColumn(nRow, "trgtAtribSbst"	, "안내");
		this.ds_bfacChkDisplay.setColumn(nRow, "resltCd"		, "Y");						
		this.ds_bfacChkDisplay.setColumn(nRow, "ruleNm"			, "주의요함고객");
		this.ds_bfacChkDisplay.setColumn(nRow, "ruleId"			, "ICG_0001");
	}	

	//사전체크 dataset 추가 END ->
	
	//입력LOCK
	this.GC_INPUT_LOCK = "Y";		
	
	//사전체크LOCK
	this.GC_CHECK_LOCK = "Y";
			
	//기존단말기변제약사항체크 
	this.fn_checkBefIntmIcgLmt();	
				
	//컴포넌트 초기화
	this.ofn_initComp();
		
}
/**
