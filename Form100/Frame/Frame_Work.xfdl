			this.gfn_initDS(this.ds_bfacChkDisplay, false);
			//임시저장 Dataset Clear		
			this.gfn_initDS(this.ds_icgTmpInfoOut, false);		
			
			//모든 컴포넌트 Enable/Disable
			this.ofn_enableAllComp(false);
			//모든 컴포넌트 Data Clear
			this.ofn_clearAllCompData();		
				
			//사전체크 버튼 
			this.gfn_enableCtrls(this, "btn_bfacChk", "btn_dtlReslt");	
		}else {
			obj.set_value(e.prevalue);
			return;
		}
	}
	
	if(obj.value == this.GC_ICG_TYPE_54) {
		this.gfn_showCtrls(this, "div_insur", "div_spnsr");	
		this.div_insur.set_left(475);		
	}else {
		this.gfn_showCtrls(this, "div_spnsr", "div_insur");	
		this.div_spnsr.set_left(475);
	}	
	
	//단말기할부 
	if(obj.value == this.GC_ICG_TYPE_01 || obj.value == this.GC_ICG_TYPE_17) {	//우수고객기변 || 휴대폰기변
		this.gfn_showCtrls(this, "chk_hndsetInsl");
		this.chk_hndsetInsl.set_value(true);
	}else {
		this.gfn_showCtrls(this, "", "chk_hndsetInsl");
		this.chk_hndsetInsl.set_value(false);		
	}

	//개인정보활용 
	if(obj.value == this.GC_ICG_TYPE_01) {	//우수고객기변		
		this.gfn_showCtrls(this, "sta_indvInfoYn,rdo_indvInfoYn,btn_indvInfoYn");
		this.rdo_indvInfoYn.set_value("Y");
	}else {
		this.gfn_showCtrls(this, "", "sta_indvInfoYn,rdo_indvInfoYn,btn_indvInfoYn");
		this.rdo_indvInfoYn.set_value();
	}		

}
