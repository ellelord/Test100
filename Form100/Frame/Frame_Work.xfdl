/**
 * @description	판매번호 변경시 
 */
this.edt_saleNo_onchanged = function(obj:Edit, e:nexacro.ChangeEventInfo)
{	
	//스폰서 comp 초기화
	this.ofn_initSpnsrComp();	
	
	//스폰서 Data clear
	this.ofn_clearSpnsrData();
		
	this.gfn_initDS(this.ds_mblSalePlcyInfoIn , false);
	this.gfn_initDS(this.ds_mblSalePlcyInfo	  , false);	
	
	//수납대상금액 계산	
	this.ofn_payAmount(); 	
	
	if(!this.gfn_isNull(this.div_intm.edt_hndsetModelId.text) ) {
		//기기판매가조회 - 단말기 
		this.fn_retvIntmSalePricHndset();		
	}
	
	if(this.gfn_isNull(obj.text)) {
	
		this.div_intm.edt_saleNm.set_value();
		obj.setFocus();
		return;
	}
	
	if(!this.gfn_isEngNumOnly(obj.text) ) {
		this.gfn_showMsgBox("NBSS-I000", "영문과 숫자만 입력 가능합니다.");
		return;
	}	
	
	if(this.gfn_length(obj.text) != 5) {
		this.gfn_showMsgBox("NBSS-I000", "판매번호 맞지 않습니다.");
		
		this.div_intm.edt_saleNm.set_value();
		obj.setFocus();
		return;
	}
	
	if(this.gfn_subStr(obj.text,0,1) == "G" && application.G_USER_ENTR_PRSN_INFO == this.GC_USER_ENTR_PRSN 
			&& this.ds_orgInfoDtlLogin.getColumn(0, "ktOpnCentrYn") == "Y") {
		this.gfn_showMsgBox("NBSS-I000", "KT센터는 분납 판매번호만 선택 가능 합니다. 확인하세요.");

		this.div_intm.edt_saleNm.set_value();
		obj.setFocus();
		return;
	}

	//판매정책정보조회 
	this.fn_retvSalePlcyInfo(obj.text);
}

