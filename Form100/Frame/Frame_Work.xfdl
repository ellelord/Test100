/**
 * @description	판매정보 CALLBACK
 */
this.fn_callbackSaleInfo = function(divCd)
{
	switch (divCd) 
	{
		//판매점
		case "org":
			if(!this.gfn_isNull(this.div_sale.edt_salorgId.text)) {
			
				//판매점 체크
				if(application.G_ORG_ID == "MNS1101") {
					
					if(application.G_ORG_ID != this.div_sale.edt_salorgId.text) {
						var sOrgIdPre = this.gfn_subStr(this.div_sale.edt_salorgId.text,0,2);
						if(!(sOrgIdPre == "YC" || sOrgIdPre == "YD" || sOrgIdPre == "YM" || sOrgIdPre == "YH"
								|| sOrgIdPre == "YS" || sOrgIdPre == "YO" || sOrgIdPre == "YG" || sOrgIdPre == "YR"
								|| sOrgIdPre == "YF") ) {
						
							this.gfn_showMsgBox("NBSS-I000", "로그인 조직이 MNS1101인경우 YC/YD/YM/YH/YS/YO/YG/YR/YF  가 아니면 입력 불가합니다.");
							this.div_sale.edt_salorgId.set_value();
							this.div_sale.edt_salorgNm.set_value();
							return false;			
						}	
					}
				}				
				if(application.G_ORG_ID == "YA2") {	
					var sOrgIdPre = this.gfn_subStr(this.div_sale.edt_salorgId.text,0,3);
					if(!(sOrgIdPre != "YA2") ) {			
						this.gfn_showMsgBox("NBSS-I000", "로그인 조직이 GTS인경우 YA2가 아니면 입력 불가합니다.");
						this.div_sale.edt_salorgId.set_value();
						this.div_sale.edt_salorgNm.set_value();
						return false;			
					}		
				}	
	
				//조직ID로 상세조회 - 판매점ID
				var rtnDS = this.ofn_getOrgDtl(this.div_sale.edt_salorgId.text);
				this.ds_orgInfoDtl.assign(rtnDS);
				//trace("===>\n"+this.ds_orgInfoDtl.saveXML());				 
				
				if(this.gfn_isNull(rtnDS) || this.ds_orgInfoDtl.rowcount <= 0) {
					//판매점관련 comp 초기화
					this.div_sale.edt_salorgId.set_value();
					this.div_sale.edt_salorgNm.set_value();
				
					this.gfn_showMsgBox("NBSS-I000", "검색된 결과가 없습니다.");
					return;					
				}else {
									
					//판매점관련 comp 초기화
					this.div_sale.edt_salorgId.set_value();
					this.div_sale.edt_salorgNm.set_value();	
	
					this.div_sale.edt_salorgId.set_value(this.ds_orgInfoDtl.getColumn(0, "orgId"));
					this.div_sale.edt_salorgNm.set_value(this.ds_orgInfoDtl.getColumn(0, "orgNm"));					
				}			
			}
			break;
			
		//접점 
		case "cpnt":
//trace("====>\n"+this.div_sale.edt_cpntId.text);		
/*		
			if(this.gfn_isNull(this.div_sale.edt_cpntId.text)) {
			
				//접점 관련 comp 초기화
				this.div_sale.edt_cpntId.set_value();
				this.div_sale.edt_cpntNm.set_value();
			
				return;	
			}	
*/							
			break;					
		
		//판매직원
		case "emp01":

			if(this.gfn_isNull(this.div_sale.edt_salerId.text)) {
			
				//판매직원 관련 comp 초기화
				this.div_sale.edt_salerId.set_value();
				this.div_sale.edt_salerNm.set_value();
			
				this.gfn_showMsgBox("NBSS-I000", "검색된 결과가 없습니다.");
				return;	
			}					
			break;	
			
		//서포트
		case "emp02":

			if(this.gfn_isNull(this.div_sale.edt_sptrId.text)) {
			
				//서포트 관련 comp 초기화
				this.div_sale.edt_sptrId.set_value();
				this.div_sale.edt_sptrNm.set_value();
			
				this.gfn_showMsgBox("NBSS-I000", "검색된 결과가 없습니다.");
				return;	
			}					
			break;				

		default:
			break;
	}
}
